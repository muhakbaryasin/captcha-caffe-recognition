net: "network_captchas_with_3_convolutional_layers_train.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
# The learning rate policy
lr_policy: "inv"
gamma: 0.0001
power: 0.75
# Display every 1000 iterations
display: 500
# The maximum number of iterations
max_iter: 50000
# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "temp/snapshots/train"
# solver mode: CPU or GPU
solver_mode: GPU
